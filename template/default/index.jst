<%@ page file="index.html" %>
<%
header("Namespaces");

for (var i = 0, tag, tags = jsd.allNamespaces.containers(); tag = tags[i]; i++) {
    var ns = tag.value || JSD.globalsNamespace;
    var file = ns.replace(/\./, "/") + ".html";
%>
    <h3><a href="<%= file %>"><%= ns %></a></h3>
    <p><%= Utl.firstSentence(tag.text, 100) %></p>
<%
}

footer();
%>
